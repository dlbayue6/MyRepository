.listNone button::after{border: 0;}
.ws_listInfo button.delbutton{margin-top:35rpx; margin-right: -30rpx; line-height: 38rpx;display: block;background:#ffb921;border-radius:12rpx;width:180rpx;font-size:30rpx;color:#fff;border:0;outline:none;}
.ws_listInfo button.delbutton2{margin-top:35rpx; margin-right: -30rpx; line-height: 38rpx;display: block;background:#ccc;border-radius:12rpx;width:200rpx;font-size:30rpx;color:#aaaaaa;border:0;outline:none;}


